                         "dotenv":  "^16.4.5",
                         "express":  "^4.18.3",
                         "express-rate-limit":  "^7.1.5",
                         "helmet":  "^7.1.0",
                         "http-status-codes":  "^2.3.0",
                         "jsonwebtoken":  "^9.0.2",
                         "morgan":  "^1.10.0",
                         "nanoid":  "^4.0.2",
                         "zod":  "^3.22.4"
                     },
    "devDependencies":  {
                            "@types/compression":  "^1.7.5",
