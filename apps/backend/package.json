    "private":  true,
    "type":  "module",
    "scripts":  {
                    "dev":  "tsx watch --clear-screen=false src/server.ts",
                    "build":  "tsc -p tsconfig.build.json",
                    "start":  "node dist/server.js",
                    "lint":  "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
